<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="NormalLabelStyle" TargetType="{x:Type Label}">
        <Setter Property="FontSize" Value="12"/>
        <Setter Property="FontFamily" Value="Kaiti"/>
    </Style>

    <Style x:Key="IconLabelStyle" TargetType="{x:Type Label}">
        <Setter Property="FontSize" Value="32"/>
        <Setter Property="FontFamily" Value="..\Resources\Iconfont\#iconfont"/>
    </Style>
</ResourceDictionary>